    .popUpTxt {
      white-space: normal;
      overflow: unset;
      text-overflow: unset;
      display: block;
      width: auto;
      flex-wrap: wrap;

    }

    .headings {
      margin-bottom: 60px;
    }


    .form-control {
      border-radius: 1.5em;
    }

    .conference-header {
      margin-top: 20px;
    }




