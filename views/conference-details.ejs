  <%- include('includes/navigation.ejs') %>


  <p align="center" class="sign head headings">Session details</p>


  <div class="main1">
    <article class="active">
      <div class="circular--landscape">
        <img src="<%=conference.speakerImg%>" class="img" alt="Speaker Photo">
      </div>
      <div class="text">
        <p class="sign conference-header" align="center"><%=conference.name%></p>
        <p maxlenght="20"><b>Description: </b><%=conference.description%></p>
        <p><b>Start time: </b><%=conference.startTime.toString().substring(0, 21)%></p>
        <p><b>End time: </b><%=conference.endTime.toString().substring(0, 21) %></p>
        <p><b>Address: </b><%=conference.address.adress%></p>
        <p><b> Speaker name: </b><%=conference.speakerName%></p>

      </div>
    </article>
  </div>





  <div class=sign1>
    <label class="sign head headings">Adding sessions</label>
  </div>


  <div class="main" style="margin-top: 0px;">
    <form class=" product-form form1" action="/add-session" method="POST">

      <p class="sign2">Address</p>
      <p class="sign1" style="color:black;"><%= conference.address.adress %></p>
      <input class="name" type="hidden" name="venueId" value="<%= conference.address._id %>">
      <input class="name" type="hidden" name="conferenceId" value="<%= conference._id %>">

      <p class="sign2">Choose hall</p>

      <select class="role description form-control" name="hallId" id="hall">
        <% for (let hall of halls) { %>
        <% if(hall.venueId.toString() === conference.address._id.toString()) { %>
        <option value="<%=hall._id%>"><%= hall.name %> - Seats capacity: <%= hall.seats %> </option>
        <% } %>
        <% } %>
      </select>
      <label class="sign2" align="center" for="startTime">Choose start end date and time</label>
      <input class="name" type="datetime-local" name="startTime" id="startTime">
      <label class="sign2" align="center" for="endTime">Choose start end date and time</label>
      <input class="name" type="datetime-local" name="endTime" id="endTime">
      <button class="submit btn btn-block" type="submit">Add Session</button>
    </form>
  </div>

  <div class=sign1>
    <label class="sign head headings">Sessions</label>
  </div>

  <div class="buttons-data-filter" id="myBtnContainer">

    <button class="submit1 btn btn-session allSessionsDisplayBtn active" type="button" data-toggle="collapse"
      data-target="#allSessionsDisplay" aria-expanded="false" aria-controls="allSessionsDisplay">ALL</button>

    <button class="submit1 btn btn-session pastSessionsDisplayBtn" type="button" data-toggle="collapse"
      data-target="#pastSessionsDisplay" aria-expanded="false" aria-controls="pastSessionsDisplay">PAST</button>

    <button class="submit1 btn btn-session activeSessionsDisplayBtn" type="button" data-toggle="collapse"
      data-target="#activeSessionsDisplay" aria-expanded="false" aria-controls="activeSessionsDisplay">ACTIVE</button>

    <button class="submit1 btn btn-session upcommingSessionsDisplayBtn" type="button" data-toggle="collapse"
      data-target="#upcommingSessionsDisplay" aria-expanded="false"
      aria-controls="upcommingSessionsDisplay">UPCOMING</button>
  </div>


  <div class="grid collapse allSessionsDisplay" id="allSessionsDisplay">
    <% for(let session of allSessions) { %>
    <div class="main allSessions">
      <article class="sessions">
        <h3><b class="sign2">Hall: </b><%=session.hallId.name%></h3>
        <h3><b class="sign2">Seats available: </b><%=session.sessionSeats%></h3>
        <p class="pStartTime"><b class="sign2">Start time: </b><%=session.startTime%></p>
        <p class="pEndTime"><b class="sign2">End time: </b><%=session.endTime%></p>
        <form class="product-form form1" action="/join-session" method="POST">
          <input type="hidden" value="<%= session._id %>" name="sessionId" />
          <input type="hidden" value="<%= conference._id %>" name="conferenceId" />
          <button class="submit btn btn-block" type="submit">Join session</button>
        </form>
      </article>
    </div>
    <% } %>
  </div>

  <div class="grid collapse pastSessionsDisplay" id="pastSessionsDisplay">
    <% for(let session of pastSessions) { %>
    <div class="main1 filterDiv allSessions">
      <article class="sessions">
